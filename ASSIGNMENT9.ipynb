    "from point import Point\n",
    "from circle import Circle\n",
    "\n",
    "center = Point(2, 3)\n",
    "circle = Circle(center, 5)\n",
    "\n",
    "p = Point(4, 6)\n",
    "\n",
    "print(circle)\n",
    "print(\"Area:\", circle.area())\n",
    "print(\"Circumference:\", circle.circumference())\n",
    "print(\"Point inside circle:\", circle.contains(p))"
   ]
